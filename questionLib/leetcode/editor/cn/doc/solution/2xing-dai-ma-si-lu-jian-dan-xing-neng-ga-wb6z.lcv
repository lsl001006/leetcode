class Solution(object):
    def arrayRankTransform(self, arr):
        """
        :type arr: List[int]
        :rtype: List[int]
        """
        num_map = {num:idx  for idx,num in enumerate(sorted(set(arr)),1)}
        return [num_map[num] for num in arr]

